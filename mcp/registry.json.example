{
  "servers": {
    "duckduckgo": {
      "image": "mcp/duckduckgo",
      "scopes": ["search"],
      "allow_clients": ["*"],
      "rate_limit_rpm": 60,
      "timeout_sec": 30,
      "env_schema": {}
    },
    "github-official": {
      "image": "mcp/github-official",
      "scopes": ["github"],
      "allow_clients": ["*"],
      "env_schema": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "required"
      }
    }
  }
}
